  "permissions": [
    "homey:manager:api"
  ],
  "flow": {
    "actions": [
      {
        "id": "archive_all",
        "title": {
          "en": "Export all insights",
          "nl": "Exporteer alle insights"
        },
        "args": [
          {
            "name": "resolution",
            "type": "dropdown",
            "values": [
              {
                "id": "lastHour",
                "label": {
                  "en": "last hour"
                }
              },
              {
                "id": "last6Hours",
                "label": {
                  "en": "last 6 hours"
                }
              },
              {
                "id": "last24Hours",
                "label": {
                  "en": "last 24 hours"
                }
              },
              {
                "id": "last7Days",
                "label": {
                  "en": "last 7 days"
                }
              },
              {
                "id": "last14Days",
                "label": {
                  "en": "last 14 days"
                }
              },
              {
                "id": "last31Days",
                "label": {
                  "en": "last 31 days"
                }
              },
              {
                "id": "today",
                "label": {
                  "en": "today"
                }
              },
              {
                "id": "yesterday",
                "label": {
                  "en": "yesterday"
                }
              },
              {
                "id": "thisWeek",
                "label": {
                  "en": "this week"
                }
              },
              {
                "id": "lastWeek",
                "label": {
                  "en": "last week"
                }
              },
              {
                "id": "thisMonth",
                "label": {
                  "en": "this month"
                }
              },
              {
                "id": "lastMonth",
                "label": {
                  "en": "last month"
                }
              },
              {
                "id": "thisYear",
                "label": {
                  "en": "this year"
                }
              },
              {
                "id": "lastYear",
                "label": {
                  "en": "last year"
                }
              },
              {
                "id": "last2Years",
                "label": {
                  "en": "last 2 years"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "archive_app",
        "title": {
          "en": "Export insights of:",
          "nl": "Exporteer insights van:"
        },
        "args": [
          {
            "name": "selectedApp",
            "type": "autocomplete"
          },
          {
            "name": "resolution",
            "type": "dropdown",
            "values": [
              {
                "id": "lastHour",
                "label": {
                  "en": "last hour"
                }
              },
              {
                "id": "last6Hours",
                "label": {
                  "en": "last 6 hours"
                }
              },
              {
                "id": "last24Hours",
                "label": {
                  "en": "last 24 hours"
                }
              },
              {
                "id": "last7Days",
                "label": {
                  "en": "last 7 days"
                }
              },
              {
                "id": "last14Days",
                "label": {
                  "en": "last 14 days"
                }
              },
              {
                "id": "last31Days",
                "label": {
                  "en": "last 31 days"
                }
              },
              {
                "id": "today",
                "label": {
                  "en": "today"
                }
              },
              {
                "id": "yesterday",
                "label": {
                  "en": "yesterday"
                }
              },
              {
                "id": "thisWeek",
                "label": {
                  "en": "this week"
                }
              },
              {
                "id": "lastWeek",
                "label": {
                  "en": "last week"
                }
              },
              {
                "id": "thisMonth",
                "label": {
                  "en": "this month"
                }
              },
              {
                "id": "lastMonth",
                "label": {
                  "en": "last month"
                }
              },
              {
                "id": "thisYear",
                "label": {
                  "en": "this year"
                }
              },
              {
                "id": "lastYear",
                "label": {
                  "en": "last year"
                }
              },
              {
                "id": "last2Years",
                "label": {
                  "en": "last 2 years"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "purge",
        "title": {
          "en": "Purge old files and folders",
          "nl": "Verwijder oude bestanden en mappen"
        },
        "args": [
          {
            "name": "storage",
            "type": "dropdown",
            "values": [
              {
                "id": "FTP",
                "label": {
                  "en": "FTP"
                }
              },
              {
                "id": "SMB",
                "label": {
                  "en": "NAS/SMB"
                }
              }
            ]
          },
          {
            "name": "types",
            "type": "dropdown",
            "values": [
              {
                "id": "onlyZip",
                "label": {
                  "en": "only ZIP files"
                }
              },
              {
                "id": "allTypes",
                "label": {
                  "en": "all files and folders"
                }
              }
            ]
          },
          {
            "name": "daysOld",
            "type": "number",
            "min": 1,
            "placeholder": {
              "en": "delete older then number of days"
            }
          }
        ]
      }
    ]
  }
}