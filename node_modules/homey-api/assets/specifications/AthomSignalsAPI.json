{
  "host": "signals.athom.com",
  "basePath": "/api/",
  "operations": {
    "getSignals": {
      "path": "/prontohex/search",
      "method": "post",
      "parameters": {
        "brand": {
          "type": "string",
          "in": "body"
        },
        "type": {
          "type": "string",
          "in": "body"
        },
        "query": {
          "type": "string",
          "in": "body"
        }
      }
    },
    "getSignal": {
      "path": "/prontohex/{signalId}",
      "method": "get",
      "parameters": {
        "signalId": {
          "in": "path",
          "type": "string",
          "required": true
        }
      }
    }
  }
}